{
  "extends": "./tsconfig.json",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "compilerOptions": {
    "composite": false,
    "tsBuildInfoFile": false,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["vite/client"]
  }
}
